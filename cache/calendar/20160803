\u0441\u044f \u0441\u043e\u0432\u0435\u0442\u0430 \u0438 \u043c\u043e\u043b\u0438\u0442\u0432, \u043e\u043d \u043d\u0435\u0438\u0437\u043c\u0435\u043d\u043d\u043e \n\u043d\u0430\u043f\u0440\u0430\u0432\u043b\u044f\u043b \u0438\u0445 \u043a \"\u044e\u0440\u043e\u0434\u0438\u0432\u043e\u043c\u0443 \u0421\u0438\u043c\u0435\u043e\u043d\u0443\", \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \n\u043c\u043e\u0433 \u043b\u0443\u0447\u0448\u0435 \u043f\u0440\u0435\u043f\u043e\u0434\u0430\u0442\u044c \u0441\u043e\u0432\u0435\u0442 \u0434\u0443\u0445\u043e\u0432\u043d\u044b\u0439. \u0417\u0430 \u0442\u0440\u0438 \u0434\u043d\u044f \u0434\u043e \n\u0441\u0432\u043e\u0435\u0439 \u0441\u043c\u0435\u0440\u0442\u0438 \u0441\u0432\u044f\u0442\u043e\u0439 \u0421\u0438\u043c\u0435\u043e\u043d \u043f\u0435\u0440\u0435\u0441\u0442\u0430\u043b \u043f\u043e\u044f\u0432\u043b\u044f\u0442\u044c\u0441\u044f \n\u043d\u0430 \u0443\u043b\u0438\u0446\u0430\u0445, \u0437\u0430\u0442\u0432\u043e\u0440\u0438\u043b\u0441\u044f \u0432 \u0441\u0432\u043e\u0435\u0439 \u0445\u0438\u0436\u0438\u043d\u0435, \u0432 \u043a\u043e\u0442\u043e\u0440\u043e\u0439, \n\u043a\u0440\u043e\u043c\u0435 \u0432\u044f\u0437\u0430\u043d\u043a\u0438 \u0445\u0432\u043e\u0440\u043e\u0441\u0442\u0430, \u043d\u0438\u0447\u0435\u0433\u043e \u043d\u0435 \u0431\u044b\u043b\u043e. \u041f\u0440\u043e\u0431\u044b\u0432 \u0432 \n\u043d\u0435\u043f\u0440\u0435\u0441\u0442\u0430\u043d\u043d\u043e\u0439 \u043c\u043e\u043b\u0438\u0442\u0432\u0435 \u0442\u0440\u0438 \u0434\u043d\u044f, \u0441\u0432\u044f\u0442\u043e\u0439 \u0421\u0438\u043c\u0435\u043e\u043d \n\u043f\u0440\u0435\u0441\u0442\u0430\u0432\u0438\u043b\u0441\u044f \u043a\u043e \u0413\u043e\u0441\u043f\u043e\u0434\u0443. \u041d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0438\u0437 \u0433\u043e\u0440\u043e\u0434\u0441\u043a\u0438\u0445 \n\u043d\u0438\u0449\u0438\u0445, \u0434\u0440\u0443\u0436\u0438\u0432\u0448\u0438\u0445 \u0441 \u043d\u0438\u043c, \u043d\u0435 \u0432\u0441